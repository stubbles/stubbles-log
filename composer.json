{
    "name": "stubbles/log",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stubbles Log provides support for writing log data.",
    "require": {
        "php": ">=5.4.0",
        "stubbles/core": "~5.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.3",
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\log\\": "src/main/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
